-doAdd()...start--------------------------\");\n      var max = this.TaskList.reduce(function (a, b) {\n        return a > b.id ? a : b.id;\n      }, 0);\n      this.TaskList.push({\n        id: max + 1,\n        check: false,\n        content: this.p_content,\n        date: this.p_date\n      }); //console.log(\"TaskList_length \"+this.TaskList.length)\n    },\n    deleteList: function (index) {\n      this.TaskList.splice(index, 1); //console.log(\"TaskList_length \"+this.TaskList.length)\n\n      this.notdelete = false;\n    },\n    getcheckbox: function (e) {\n      this.notdelete = false;\n      console.log(\"getcheckbox start\" + e);\n    }\n  }\n};","map":{"version":3,"mappings":"AAqCA,eAAe;EACXA,KAAI,EAAI,CAAC,WAAD,EAAc,QAAd,EAAwB,cAAxB,CADG;EAEXC,KAAI,EAAI;IACJC,YAAY,GAAE;MACV,KAAKC,QAAL,GAAgB,EAAhB;MACAC,OAAO,CAACC,GAAR,CAAY,4BAAZ;MACA,KAAKC,SAAL,GAAiB,KAAjB;IACH,CALG;;IAMJH,QAAO,EAAI;MACPI,OAAO,GAAG;QACN,KAAKC,KAAL,CAAW,YAAX,EAAyB,KAAKL,QAAL,CAAcM,MAAvC;QACAL,OAAO,CAACC,GAAR,CAAY,6BAA2B,KAAKF,QAAL,CAAcM,MAArD;MACH,CAJM;;MAKPC,IAAI,EAAE;IALC;EANP,CAFG;EAgBXC,IAAG,EAAI,YAAU;IACb,OAAM;MACFC,GAAE,EAAI,EADJ;MAEFN,SAAQ,EAAI,IAFV;MAGFH,QAAO,EAAI,CACP;QAACU,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,UAAnC;QAA+CC,IAAI,EAAC;MAApD,CADO,EAEP;QAACH,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,OAAnC;QAA4CC,IAAI,EAAC;MAAjD,CAFO,EAGP;QAACH,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,SAAnC;QAA8CC,IAAI,EAAC;MAAnD,CAHO;IAHT,CAAN;EAUH,CA3BU;;EA4BXC,YAAY,GAAE;IACVb,OAAO,CAACC,GAAR,CAAY,cAAZ;EACH,CA9BU;;EA+BXa,OAAO,GAAE;IACLd,OAAO,CAACC,GAAR,CAAY,SAAZ;IACAD,OAAO,CAACC,GAAR,CAAY,+BAA6B,KAAKF,QAAL,CAAcM,MAAvD;IACAL,OAAO,CAACC,GAAR,CAAY,6BAA2B,KAAKF,QAAL,CAAcgB,SAAd,EAAvC;IACA,KAAKX,KAAL,CAAW,YAAX,EAAyB,KAAKL,QAAL,CAAcM,MAAvC;EACH,CApCU;;EAqCXW,WAAW,GAAE;IACThB,OAAO,CAACC,GAAR,CAAY,aAAZ;EACJ,CAvCW;;EAyCXgB,OAAO,GAAE;IACLjB,OAAO,CAACC,GAAR,CAAY,SAAZ;EACH,CA3CU;;EA4CXiB,YAAY,GAAE;IACVlB,OAAO,CAACC,GAAR,CAAY,wDAAZ;IACAD,OAAO,CAACC,GAAR,CAAY,cAAY,KAAKC,S