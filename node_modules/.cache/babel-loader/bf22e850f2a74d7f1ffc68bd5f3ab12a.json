{"ast":null,"code":"export default {\n  props: ['p_content', 'p_date'],\n  data: function () {\n    return {\n      delete: true,\n      TaskList: [{\n        id: 1,\n        check: false,\n        content: \"Hey Boy!\",\n        date: \"8/16 Tue.\"\n      }, {\n        id: 2,\n        check: false,\n        content: \"Hello\",\n        date: \"8/16 Tue.\"\n      }, {\n        id: 3,\n        check: false,\n        content: \"Hi hani\",\n        date: \"8/16 Tue.\"\n      }]\n    };\n  },\n\n  beforeUpdate() {\n    if (this.delete != false) {\n      this.doAdd();\n    } else {\n      this.delete = true;\n    }\n  },\n\n  methods: {\n    doAdd: function () {\n      var max = this.TaskList.reduce(function (a, b) {\n        return a > b.id ? a : b.id;\n      }, 0);\n      this.TaskList.push({\n        id: max + 1,\n        check: false,\n        content: this.p_content,\n        date: this.p_date\n      });\n    },\n    deleteList: function (index) {\n      // console.log(\"index : \"+index)\n      this.delete = false;\n      this.TaskList.splice(index, 1);\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA,eAAe;EACXA,KAAI,EAAI,CAAC,WAAD,EAAc,QAAd,CADG;EAEXC,IAAG,EAAI,YAAU;IACb,OAAM;MACFC,MAAK,EAAI,IADP;MAEFC,QAAO,EAAI,CACP;QAACC,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,UAAnC;QAA+CC,IAAI,EAAC;MAApD,CADO,EAEP;QAACH,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,OAAnC;QAA4CC,IAAI,EAAC;MAAjD,CAFO,EAGP;QAACH,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,SAAnC;QAA8CC,IAAI,EAAC;MAAnD,CAHO;IAFT,CAAN;EASH,CAZU;;EAaXC,YAAY,GAAE;IACV,IAAG,KAAKN,MAAL,IAAe,KAAlB,EAAwB;MACpB,KAAKO,KAAL;IACH,CAFD,MAEK;MACD,KAAKP,MAAL,GAAc,IAAd;IACJ;EACH,CAnBU;;EAoBXQ,OAAM,EAAI;IACND,KAAI,EAAI,YAAU;MACd,IAAIE,GAAE,GAAI,KAAKR,QAAL,CAAcS,MAAd,CAAqB,UAASC,CAAT,EAAWC,CAAX,EAAa;QACxC,OAAOD,CAAC,GAACC,CAAC,CAACV,EAAJ,GAASS,CAAT,GAAWC,CAAC,CAACV,EAApB;MACH,CAFS,EAEP,CAFO,CAAV;MAGA,KAAKD,QAAL,CAAcY,IAAd,CAAmB;QACfX,EAAC,EAAIO,GAAG,GAAC,CADM;QACHN,KAAI,EAAI,KADL;QACYC,OAAM,EAAI,