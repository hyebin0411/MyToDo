..ing===================\\n\");\n    console.log(this.val + \"\\n\");\n  },\n\n  methods: {\n    doAdd: function () {\n      console.log(\"\\n-----------------doAdd()...start--------------------------\");\n      var max = this.TaskList.reduce(function (a, b) {\n        return a > b.id ? a : b.id;\n      }, 0);\n      this.TaskList.push({\n        id: max + 1,\n        check: false,\n        content: this.p_content,\n        date: this.p_date\n      }); //console.log(\"TaskList_length \"+this.TaskList.length)\n    },\n    deleteList: function (index) {\n      this.TaskList.splice(index, 1);\n      this.notdelete = false;\n      this.TaskList.push();\n      console.log(\"deleteList start\");\n    },\n    getcheckbox: function (e) {\n      this.notdelete = false;\n      this.font_no = true;\n      console.log(\"getcheckbox start\" + e);\n      console.log(\"font_no : \" + this.font_no);\n    }\n  }\n};","map":{"version":3,"mappings":"AAkCA,eAAe;EACXA,KAAI,EAAI,CAAC,WAAD,EAAc,QAAd,EAAwB,cAAxB,CADG;EAEXC,KAAI,EAAI;IACJC,YAAY,GAAE;MACV,KAAKC,QAAL,GAAgB,EAAhB;MACAC,OAAO,CAACC,GAAR,CAAY,4BAAZ;MACA,KAAKC,SAAL,GAAiB,KAAjB;IACH,CALG;;IAMJH,QAAO,EAAI;MACPI,OAAO,GAAG;QACN,KAAKC,KAAL,CAAW,YAAX,EAAyB,KAAKL,QAAL,CAAcM,MAAvC;QACAL,OAAO,CAACC,GAAR,CAAY,6BAA2B,KAAKF,QAAL,CAAcM,MAArD;MACH,CAJM;;MAKPC,IAAI,EAAE;IALC;EANP,CAFG;EAgBXC,IAAG,EAAI,YAAU;IACb,OAAM;MACFC,GAAE,EAAI,EADJ;MAEFN,SAAQ,EAAI,IAFV;MAGFO,OAAM,EAAI,KAHR;MAIFV,QAAO,EAAI,CACP;QAACW,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,UAAnC;QAA+CC,IAAI,EAAC;MAApD,CADO,EAEP;QAACH,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,OAAnC;QAA4CC,IAAI,EAAC;MAAjD,CAFO,EAGP;QAACH,EAAC,EAAI,CAAN;QAASC,KAAI,EAAI,KAAjB;QAAyBC,OAAM,EAAI,SAAnC;QAA8CC,IAAI,EAAC;MAAnD,CAHO;IAJT,CAAN;EAWH,CA5BU;;EA6BXC,YAAY,GAAE;IACVd,OAAO,CAACC,GAAR,CAAY,cAAZ;EACH,CA/BU;;EAgCXc,OAAO,GAAE;IACLf,OAAO,CAACC,GAAR,CAAY,SAAZ;IACAD,OAAO,CAACC,GAAR,CAAY,+BAA6B,KAAKF,QAAL,CAAcM,MAAvD;IACAL,OAAO,CAACC,GAAR,CAAY,6BAA2Be,IAAI,CAACC,SAAL,CAAe,KAAKlB,QAApB,CAAvC;I